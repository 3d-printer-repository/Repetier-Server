<printer>
    <general>
        <name>iRapid</name>
        <slug>irapid</slug>
        <printerVariant>cartesian</printerVariant>
        <active>true</active>
        <fan>false</fan>
        <tempUpdateEvery>1</tempUpdateEvery>
        <softwarePower>false</softwarePower>
        <sdcard>false</sdcard>
	</general>
    <connection>
        <serial>
            <baudrate>115200</baudrate>
            <device>/dev/tty.usbmodemfa1341</device>
            <pingPong>false</pingPong>
            <inputBufferSize>127</inputBufferSize>
            <protocol>0</protocol>
            <okAfterResend>true</okAfterResend>
        </serial>
    </connection>
    <shape>
        <grid color="#000000" spacing="10"/>
        
	
	<rectangle color="#808080" xMax="100" xMin="0" yMax="100" yMin="0"/>
	</shape>
    <movement>
        <xMin>0</xMin>
        <xMax>100</xMax>
        <xHome>0</xHome>
        <yMin>0</yMin>
        <yMax>100</yMax>
        <yHome>0</yHome>
        <zMin>0</zMin>
        <zMax>120</zMax>
        <zHome>0</zHome>
        <xyJerk>40</xyJerk>
        <zJerk>0.3</zJerk>
        <xyPrintAcceleration>1500</xyPrintAcceleration>
        <xyTravelAcceleration>2500</xyTravelAcceleration>
        <xySpeed>100</xySpeed>
        <zSpeed>2</zSpeed>
        <endstops all="true" x="true" y="true" z="true"/>
    </movement>
    <extruders>
		<extruder lastTemp="185" num="0">
			<temperatures>
				<temperature value="185">PLA 185°C</temperature>
				<temperature value="190">PLA 190°C</temperature>
				<temperature value="200">PLA 200°C</temperature>
				<temperature value="205">PLA 205°C</temperature>
				<temperature value="210">PLA 210°C</temperature>
			</temperatures>
		</extruder>
	</extruders>
    <heatedBed installed="true" lastTemp="1">
        <temperatures>
			<temperature value="60">PLA 60°C</temperature>
			<temperature value="110">ABS 110°C</temperature>
		</temperatures>
    </heatedBed>
